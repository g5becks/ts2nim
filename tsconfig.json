{
  "compilerOptions": {
  "module": "commonjs",
  "strict": true,
  "target": "ES2017",
  "esModuleInterop": true,
  "lib": [
    "ESNext",
  ],
  "sourceMap": true,
  "noImplicitReturns": true,
  "noUnusedLocals": true,
  "noUnusedParameters": true,
  "pretty": true
},
"exclude": [
  "node_modules",
  "**/*.test.ts"
],
"include": [
  "src/**/*"
]
}
